{
    "directories": [
        "src/"
    ],
    "extract": true,
    "files": [
        "LICENSE",
        "README.md",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": [
                "*.php",
                "*.json",
                "*.exe"
            ],
            "exclude": [
                "herrera-io",
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "vendor"
        }
    ],
    "replacements": {
        "manifest_url": "https://raw.githubusercontent.com/TheRatG/sfmysqlbackup/gh-pages/manifest.json",
        "phar_url": "https://github.com/TheRatG/sfmysqlbackup/releases/download/1.0.0/sfmysqlbackup.phar"
    },
    "compression": "GZ",
    "git-commit": "git-commit",
    "git-version": "package_version",
    "main": "bin/sfmysqlbackup",
    "output": "build/sfmysqlbackup.phar",
    "stub": true,
    "chmod": "0755"
}